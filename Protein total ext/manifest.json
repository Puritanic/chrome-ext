{
    "manifest_version": 2,

    "name": "Protein Total",
    "description": "An protein tracker app",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "options_page": "options.html",
    
    "background": {
        "scripts": ["eventpage.js"],
        "persistent": false
    },

    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ]
}